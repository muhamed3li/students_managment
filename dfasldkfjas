[32m+[m[32mfunction studentDualBox(studentsList) {[m
[32m+[m[32m    $("#barcode").on("keypress", function (event) {[m
[32m+[m[32m        var keycode = event.keyCode ? event.keyCode : event.which;[m
[32m+[m[32m        if (keycode == "13") {[m
[32m+[m[32m            attendAjax(event, studentsList);[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m[32m}[m
[32m+[m[32mfunction attendAjax(e, studentsList) {[m
[32m+[m[32m    e.preventDefault();[m
[32m+[m
[32m+[m[32m    var id = $("#barcode").val();[m
[32m+[m[32m    id = idFromBarcode(id);[m
[32m+[m
[32m+[m[32m    $.ajax({[m
[32m+[m[32m        type: "GET",[m
[32m+[m[32m        url: "/students/getStudetnById/" + id,[m
[32m+[m[32m        data: { _token: "{{csrf_token()}}" },[m
[32m+[m[32m        success: function (data) {[m
[32m+[m[32m            let student = JSON.parse(data);[m
[32m+[m[32m            studentsList.append(`[m
[32m+[m[32m            <option value="${student.id}" selected>${student.name}</option>[m
[32m+[m[32m            `);[m
[32m+[m[32m            studentsList.bootstrapDualListbox("refresh");[m
[32m+[m[32m            $("#barcode").val("");[m
[32m+[m[32m            $("#barcode").focus();[m
[32m+[m[32m        },[m
[32m+[m[32m        error: function (data, textStatus, errorThrown) {[m
[32m+[m[32m            console.log(data);[m
[32m+[m[32m        },[m
[32m+[m[32m    });[m
[32m+[m[32m}[m
